<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(custom_list) -%>">
  <span class='title'>
    <%= custom_list.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.custom_lists_custom_list_path(custom_list),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_custom_lists_admin_custom_list_path(custom_list),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.custom_lists_admin_custom_list_path(custom_list),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => custom_list.title),
        :method => :delete %>
  </span>
</li>
